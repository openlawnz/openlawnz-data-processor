        "@openlawnz/openlawnz-parsers": "^1.0.2",
        "cli-progress": "^3.11.2",
        "dotenv": "^16.0.3",
        "inquirer": "^9.1.4",
        "pg": "^8.8.0",
        "yargs": "^17.6.2"
    },
